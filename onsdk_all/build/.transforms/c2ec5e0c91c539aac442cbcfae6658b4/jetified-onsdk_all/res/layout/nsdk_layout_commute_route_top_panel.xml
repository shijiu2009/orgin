<?xml version="1.0" encoding="utf-8"?>
<com.baidu.navisdk.module.routeresultbase.view.support.widgit.BNFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/commute_route_top"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.baidu.navisdk.module.routeresultbase.view.support.widgit.BNRelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/navi_dimens_46dp">
        <ImageView
            android:id="@+id/commute_route_back"
            android:layout_width="@dimen/navi_dimens_46dp"
            android:layout_height="@dimen/navi_dimens_46dp"
            android:layout_marginLeft="@dimen/navi_dimens_10dp"
            android:layout_centerVertical="true"
            android:src="@drawable/nsdk_ugc_icon_back"
            android:scaleType="centerInside" />

        <RelativeLayout
            android:id="@+id/commute_setting"
            android:layout_width="@dimen/navi_dimens_46dp"
            android:layout_height="@dimen/navi_dimens_46dp"
            android:layout_marginRight="@dimen/navi_dimens_10dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true">

            <ImageView
                android:id="@+id/commute_setting_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@drawable/nsdk_drawable_commute_route_setting"
                android:scaleType="centerInside" />

            <View
                android:layout_width="1px"
                android:layout_height="@dimen/navi_dimens_46dp"
                android:layout_alignTop="@id/commute_setting_iv"
                android:layout_alignBottom="@id/commute_setting_iv"
                android:background="#DBDBDB"/>
        </RelativeLayout>

        <com.baidu.navisdk.module.routeresultbase.view.support.widgit.BNLinearLayout
            android:id="@+id/title_container"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/navi_dimens_46dp"
            android:layout_centerInParent="true"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/commute_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="回家"
                android:textColor="#000000"
                android:textSize="@dimen/navi_dimens_19dp" />

            <ImageView
                android:id="@+id/commute_triangle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="@dimen/navi_dimens_3dp"
                android:src="@drawable/nsdk_drawable_commute_triangle_selector" />
        </com.baidu.navisdk.module.routeresultbase.view.support.widgit.BNLinearLayout>
    </com.baidu.navisdk.module.routeresultbase.view.support.widgit.BNRelativeLayout>

</com.baidu.navisdk.module.routeresultbase.view.support.widgit.BNFrameLayout>