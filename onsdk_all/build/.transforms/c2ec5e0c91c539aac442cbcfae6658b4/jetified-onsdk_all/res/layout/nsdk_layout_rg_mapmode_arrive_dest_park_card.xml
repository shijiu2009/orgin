<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/bnav_rg_arrive_dest_park_root"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/nsdk_nav_notification_background"
    android:elevation="@dimen/nsdk_rg_toolbox_margin_left"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/bnav_rg_arrive_dest_park_title_ly"
        android:layout_width="match_parent"
        android:layout_height="@dimen/navi_dimens_42dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/navi_dimens_20dp"
        android:paddingRight="@dimen/navi_dimens_20dp">

        <ImageView
            android:id="@+id/bnav_rg_arrive_dest_park_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/navi_dimens_5dp"
            android:layout_centerVertical="true"
            android:src="@drawable/nsdk_drawable_arrive_dest_park_one"
            android:visibility="visible" />

        <TextView
            android:id="@+id/bnav_rg_arrive_dest_park_main_title"
            android:layout_width="@dimen/navi_dimens_0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ellipsize="end"
            android:includeFontPadding="false"
            android:maxLines="1"
            android:text="科实大厦停车场"
            android:textColor="@color/nsdk_cl_text_b_mm_title"
            android:textSize="@dimen/navi_dimens_15dp" />

        <TextView
            android:id="@+id/bnav_rg_arrive_dest_park_distance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:text="距离终点150米"
            android:textColor="@color/nsdk_rg_operable_notification_subtitle"
            android:textSize="@dimen/navi_dimens_12dp" />

    </LinearLayout>

    <com.baidu.navisdk.ui.widget.AutoWrapLineLayout
        android:id="@+id/bnav_rg_arrive_dest_park_detail_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/navi_dimens_20dp"
        android:paddingRight="@dimen/navi_dimens_20dp">

        <TextView
            android:id="@+id/bnav_rg_arrive_dest_park_left_cnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/navi_dimens_8dp"
            android:layout_marginRight="@dimen/navi_dimens_4dp"
            android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_text_bg"
            android:paddingBottom="@dimen/navi_dimens_4dp"
            android:paddingLeft="@dimen/navi_dimens_4dp"
            android:paddingRight="@dimen/navi_dimens_4dp"
            android:paddingTop="@dimen/navi_dimens_4dp"
            android:layout_marginTop="@dimen/navi_dimens_4dp"
            android:text="剩余车位：22"
            android:textColor="@color/nsdk_cl_text_b_mm"
            android:textSize="@dimen/navi_dimens_12dp" />

        <TextView
            android:id="@+id/bnav_rg_arrive_dest_park_total_cnt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/navi_dimens_8dp"
            android:layout_marginRight="@dimen/navi_dimens_4dp"
            android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_text_bg"
            android:paddingBottom="@dimen/navi_dimens_4dp"
            android:paddingLeft="@dimen/navi_dimens_4dp"
            android:paddingRight="@dimen/navi_dimens_4dp"
            android:paddingTop="@dimen/navi_dimens_4dp"
            android:layout_marginTop="@dimen/navi_dimens_4dp"
            android:text="总车位：122"
            android:textColor="@color/nsdk_cl_text_b_mm"
            android:textSize="@dimen/navi_dimens_12dp" />

        <TextView
            android:id="@+id/bnav_rg_arrive_dest_park_open_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/navi_dimens_8dp"
            android:layout_marginRight="@dimen/navi_dimens_4dp"
            android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_text_bg"
            android:paddingBottom="@dimen/navi_dimens_4dp"
            android:paddingLeft="@dimen/navi_dimens_4dp"
            android:paddingRight="@dimen/navi_dimens_4dp"
            android:paddingTop="@dimen/navi_dimens_4dp"
            android:layout_marginTop="@dimen/navi_dimens_4dp"
            android:text="24小时营业"
            android:textColor="@color/nsdk_cl_text_b_mm"
            android:textSize="@dimen/navi_dimens_12dp" />

        <TextView
            android:id="@+id/bnav_rg_arrive_dest_park_price_desc"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/navi_dimens_8dp"
            android:layout_marginRight="@dimen/navi_dimens_4dp"
            android:background="@drawable/nsdk_drawable_rg_arrive_dest_park_text_bg"
            android:paddingBottom="@dimen/navi_dimens_4dp"
            android:paddingLeft="@dimen/navi_dimens_4dp"
            android:paddingRight="@dimen/navi_dimens_4dp"
            android:paddingTop="@dimen/navi_dimens_4dp"
            android:layout_marginTop="@dimen/navi_dimens_4dp"
            android:text="车位价格：2元/小时(白天)"
            android:textColor="@color/nsdk_cl_text_b_mm"
            android:textSize="@dimen/navi_dimens_12dp" />
    </com.baidu.navisdk.ui.widget.AutoWrapLineLayout>

    <LinearLayout
        android:id="@+id/bnav_rg_arrive_dest_park_bottom_layout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/navi_dimens_66dp"
        android:divider="@drawable/nsdk_drawable_divider_medium"
        android:orientation="horizontal"
        android:paddingTop="@dimen/navi_dimens_16dp"
        android:paddingBottom="@dimen/navi_dimens_16dp"
        android:paddingLeft="@dimen/navi_dimens_9dp"
        android:paddingRight="@dimen/navi_dimens_9dp">

        <RelativeLayout
            android:id="@+id/bnav_rg_arrive_dest_park_cancel_btn_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/navi_dimens_9dp"
            android:layout_marginRight="@dimen/navi_dimens_9dp"
            android:layout_weight="1"
            android:background="@drawable/nsdk_drawable_rg_operable_notification_cancel_btn"
            android:gravity="center">

            <TextView
                android:id="@+id/bnav_rg_arrive_dest_park_cancel_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="结束导航"
                android:textColor="@color/nsdk_cl_text_b_mm"
                android:textSize="@dimen/navi_dimens_14dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/bnav_rg_arrive_dest_park_confirm_btn_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/navi_dimens_9dp"
            android:layout_marginRight="@dimen/navi_dimens_9dp"
            android:layout_weight="1"
            android:background="@drawable/nsdk_drawable_rg_operable_notification_confirm_btn"
            android:gravity="center">

            <TextView
                android:id="@+id/bnav_rg_arrive_dest_park_confirm_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="去停车场"
                android:textColor="@color/nsdk_rg_operable_notification_low_priority_confirm_text"
                android:textSize="@dimen/navi_dimens_14dp" />
        </RelativeLayout>

    </LinearLayout>
</LinearLayout>